public static void main( String [] args ) {
        
    long begin = System.currentTimeMillis();    

    for( int i=0; i<LOOP_COUNT; i++ ) {
        produceHtmlTable();    
    }

    long end = System.currentTimeMillis();    
    System.out.println( "Duration " + (end - begin) + " ms" );
        
}  // try this for time